services:
  samplewebapp:
    build:
      context: C:\Users\utkarshgoyal\Documents\pipelines
      dockerfile: SampleWebApp/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: SampleWebApp
      target: base
    container_name: SampleWebApp
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: samplewebapp:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/SampleWebApp.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - C:\Users\utkarshgoyal\Documents\pipelines\SampleWebApp:/app:rw
    - C:\Users\utkarshgoyal\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\utkarshgoyal\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\utkarshgoyal\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\utkarshgoyal\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\utkarshgoyal\Documents\pipelines:/src:rw
version: '3.4'